{
  "manifest_version": 3,
  "name": "Vimtion",
  "version": "1.0.0",
  "description": "A Chrome extension that brings Vim keybindings to Notion",
  "author": "Tatsuya Kamijo",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.notion.so/*"],
      "js": ["content_scripts/vim.js"],
      "css": ["content_scripts/vim.css"]
    }
  ]
}
