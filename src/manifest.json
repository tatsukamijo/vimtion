{
  "manifest_version": 3,
  "name": "Vim for Notion",
  "version": "0.0.1",
  "description": "Allows you to edit notion documents using vim",
  "content_scripts": [
    {
      "matches": ["*://*.notion.so/*"],
      "js": ["content_scripts/vim.js"],
      "css": ["content_scripts/vim.css"]
    }
  ]
}
